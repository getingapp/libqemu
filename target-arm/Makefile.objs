obj-$(CONFIG_LIBQEMU) += machine.o
obj-$(CONFIG_LIBQEMU) += psci.o

obj-helper-y = op_helper.o helper.o neon_helper.o iwmmxt_helper.o
obj-helper-$(TARGET_AARCH64) += helper-a64.o

obj-y += $(obj-helper-y)
